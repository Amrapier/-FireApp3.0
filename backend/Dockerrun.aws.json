{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "FROM python:3.7"
  },
  "Ports": [
    {
      "ContainerPort": 8080,
      "HostPort": 80
    }
  ],
  "Volumes": [],
  "Logging": "/var/log/",
"Command": "gunicorn -b 0.0.0.0:8080 --workers=2 --threads=2 --timeout=180 --pythonpath / application:app"
}